#!/bin/bash

mkdir vendor 2> /dev/null

unlink vendor/ruler 2> /dev/null
ln -sfr ../ruler/ vendor/ruler

unlink vendor/car 2> /dev/null
ln -sfr ../car/ vendor/car

unlink vendor/startlights 2> /dev/null
ln -sfr ../startlights/ vendor/startlights

unlink vendor/controller-box 2> /dev/null
ln -sfr ../controller-box/ vendor/controller-box

unlink vendor/startlights 2> /dev/null
ln -sfr ../startlights/ vendor/startlights
